<%= render partial: "shared/user", locals: { user: @user } %>
<div class= "bio">
  <h3>Bio</h3>
  <p><%=@user.bio%></p>
</div>
<%@posts.each do |post|%>
<%= render partial: "shared/post", locals: { post: post } %>
<% end %>
<%= link_to user_posts_path(@user) do %>
  <button type="button">See all posts</button>
<% end %>